# mount     fstype      device                  device2     fstype2     fsoptions                   fsoptions2

/boot       bml         boot
/cache      rfs         /dev/block/mmcblk0p1    NULL        ext4        NULL                        nosuid nodev check=no
/data       rfs         /dev/block/mmcblk0p2    NULL        ext4        NULL                        nosuid nodev check=no
/datadata   rfs         /dev/block/stl10        NULL        ext4        NULL                        nosuid nodev check=no
/recovery   bml         recovery
/sdcard     vfat        /dev/block/mmcblk1p1    /dev/block/mmcblk1
/system     rfs         /dev/block/stl9         NULL        ext4        NULL                        llw,check=no
